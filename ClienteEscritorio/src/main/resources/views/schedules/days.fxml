<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane styleClass="mainPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="morales.david.desktop.controllers.schedules.DaysController">
   <children>
      <TableView fx:id="daysTable" editable="true" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
        <columns>
          <TableColumn fx:id="idColumn" editable="false" minWidth="-1.0" prefWidth="-1.0" text="ID" />
            <TableColumn fx:id="nameColumn" minWidth="-1.0" prefWidth="-1.0" text="NOMBRE" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <stylesheets>
      <URL value="@../../css/table_view.css" />
      <URL value="@../../css/table.css" />
   </stylesheets>
</AnchorPane>
