<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="907.0" styleClass="mainPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="morales.david.desktop.controllers.CoursesController">
   <children>
      <TableView fx:id="coursesTable" prefHeight="600.0" prefWidth="814.0" AnchorPane.bottomAnchor="105.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
        <columns>
          <TableColumn fx:id="idColumn" editable="false" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="levelColumn" editable="false" prefWidth="75.0" text="NIVEL" />
            <TableColumn fx:id="nameColumn" editable="false" prefWidth="247.0" text="NOMBRE" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" styleClass="bottonsBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0">
         <children>
            <Button fx:id="newButton" mnemonicParsing="false" onMouseClicked="#handleButtonAction" prefHeight="50.0" prefWidth="162.0" styleClass="newButton" text="NUEVO">
               <padding>
                  <Insets bottom="15.0" left="25.0" right="25.0" top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="25.0" right="25.0" top="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="editButton" mnemonicParsing="false" onMouseClicked="#handleButtonAction" prefHeight="50.0" prefWidth="162.0" styleClass="editButton" text="EDITAR">
               <padding>
                  <Insets bottom="15.0" left="25.0" right="25.0" top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="25.0" right="25.0" top="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteButton" mnemonicParsing="false" onMouseClicked="#handleButtonAction" prefHeight="50.0" prefWidth="162.0" styleClass="deleteButton" text="ELIMINAR">
               <padding>
                  <Insets bottom="15.0" left="25.0" right="25.0" top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets bottom="25.0" top="25.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../css/teachers_view.css" />
      <URL value="@../css/table.css" />
   </stylesheets>
</AnchorPane>
